#include <stdio.h>
#include <stdlib.h>



// C recursive function to solve tower of hanoi puzzle
int Mult (int m, int m)
{
	
	// printf("Comb n=%d= r=%d=\n",n,r);

  if (n==1) { return m ;}
   
   return (m + Mult(m,n-1));
	  
 
	  
	  
  
  
}
 
int main()
{
    
    int m, n ;
	
	m=5;
	n=6;
	
	res =(m,n) ;
